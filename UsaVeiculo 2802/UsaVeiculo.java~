Class UsaVeiculo {

Public static void main (String args[])
{
  Veiculo carro = new Veiculo();
  //criando objeto e insere valor/
carro.marca = "volkswagen";
carro.modelo="fox";
carro.velocidade=80;
System.out.print(carro.marca ;)
  System.out.print(carro.modelo ;)
  System.out.print(carro.velocidade ;)
}
}
Class Veiculo {
String marca, modelo;
int velocidade;

Veiculo() {
marca="";
modelo = "";
velocidade =0;
}
}